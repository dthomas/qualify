{% extends 'base.html.twig' %}

{% block title %}Reports
{% endblock %}

{% block breadcrumb %}
    <nav class="breadcrumb" aria-label="breadcrumbs">
        <ul>
            <li><a href="{{path('home')}}">Home</a></li>
            <li><a class="is-active"href="{{ path('report.list') }}">Reports</a></li>
        </ul>
    </nav>
{% endblock %}

{% block body %}
    <div class="card">
        <div class="card-content">
            <p class="title"></p>
            <p class="title is-5 my-0">Reports</p>
        </p>
    </div>
    <div class="card-content">
        <div class="columns">
            <div class="column">
                <a class="border" href="{{ path('report.lead') }}">
                    <p class="title is-5 is-spaced has-text-link">Lead Reports</p>
                    <p class="subtitle is-6">View report about leads, their status, etc.</p>
                </a>
            </div>
        </div>
    </div>
{% endblock %}
